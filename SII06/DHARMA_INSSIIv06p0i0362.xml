				<p>
					<lb n="1"/><hi rend="grantha">svasti śrī</hi> <supplied reason="subaudible"><g type="ddanda">.</g></supplied> kacciyu n=tañcaiyuṅ koṇṭa <unclear>kan</unclear>ṉaratevarkku yāṇṭu <num value="25">2 <g type="numeral">10</g> 5</num><g type="dash"/>vatu Uttarameru<hi rend="grantha">catu<supplied reason="omitted">r</supplied>vve</hi>timaṅ<unclear>kala</unclear>ttup peruṅkuṟi <hi rend="grantha">sabhai</hi>yom Emmūrp perampalattu
					
					<lb n="2"/>muṉpiṟ teṟṟiyile pakaṟ kūṭiyiruntu Emmiṟ ceyta <hi rend="grantha">vya</hi>va<hi rend="grantha">stai</hi>yāvatu na<supplied reason="omitted">m</supplied>mūr <hi rend="grantha">mahāsabhai</hi>yomāl <hi rend="grantha">da</hi>ṇṭippuṇṭa <hi rend="grantha">duṣṭa</hi>rkaḷ <hi rend="grantha">da</hi>ṇṭamiṭṭana Uḷḷana <hi rend="grantha">da</hi><unclear>ṇṭam</unclear> Avvavāṇṭu <hi rend="grantha">grāma</hi>
					
					<lb n="3" break="no"/>kā<hi rend="grantha">rya</hi>ñ<surplus>e</surplus><supplied reason="omitted">ce</supplied>yyum perumakkaḷe <hi rend="grantha">sabhai</hi>yomaikkoṇṭu tirvārākavum tirkkamāṭṭātaṉa <hi rend="grantha">sabhai</hi>yomaiyekoṇṭu <hi rend="grantha">da</hi>ṇṭaṅka ḷiṟuttuk kuṭuppārākavum <hi rend="grantha">sabhai</hi>yiniṉṟu <hi rend="grantha">sabhāmā</hi>ṟṟam pecip pa
					
					<lb n="4" break="no"/>ṇitta tani<hi rend="grantha">purṣa</hi>raiyum <hi rend="grantha">sabhai</hi>yom paṇiyāl ceyta vāriyapperumakkaḷaiyum Aṭikkiḻ niṟppāraiyum <hi rend="grantha">madhyasta</hi>rkaḷaiyum pāṭikāppārkaḷaiyum Uḷḷiṭṭa <hi rend="grantha">sabhai</hi>ppaṇice
					
					<lb n="5" break="no"/>ytār Epperppaṭṭār<hi rend="grantha">ai</hi>yun taṇṭamiṭṭaṉa Uḷḷana taṇṭamiṭṭa Avvāṇṭukaḷ <hi rend="grantha">grāmakārya</hi>ñce<supplied reason="omitted">y</supplied>yum perumakkaḷey <hi rend="grantha">sabhai</hi>yomaikkoṇṭu tirvārākavu
					
					<lb n="6" break="no"/>m tirvuṟātana <hi rend="grantha">sabhai</hi>yomaiyekoṇṭu taṇṭam Iṟuttu kuṭuppārākavum Ipparicu ceyyārākil <hi rend="grantha">da</hi>ṇṭamiṭṭa Avvāṇṭukaḷ <hi rend="grantha">grā</hi>ma
					
					<lb n="7" break="no"/>kā<hi rend="grantha">ryya</hi>ñce<unclear>y</unclear>tār<hi rend="grantha">ai</hi> meyveṟuvak<hi rend="grantha">ai</hi> <seg rend="check">��</seg>ñ ceytukāṇan taṇṭamiṭap peruvārākavum Ittaṇṭappaṭṭu Ivva
					
					<lb n="8" break="no"/><hi rend="grantha">nyā</hi>yam vārām<hi rend="grantha">ai</hi>t tīrttukkuṭuppomānom Uttarameru<hi rend="grantha">śśatu<supplied reason="omitted">r</supplied>vve</hi>timaṅkalattu <hi rend="grantha">sabhai</hi>yom Itu <hi rend="grantha">sabhai</hi><unclear>yār</unclear>
					
					<lb n="9"/>paṇikka Eḻutineṉ <hi rend="grantha">madhyasta</hi>n <hi rend="grantha">śi</hi>va<hi rend="grantha">dāsa</hi>ṉ maṅ<hi rend="grantha">kalāditya</hi>neṉ <supplied reason="subaudible"><g type="ddanda">.</g></supplied>
