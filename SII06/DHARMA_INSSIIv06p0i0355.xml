					<lb n="1"/><hi rend="grantha">svasti śrī</hi> <supplied reason="subaudible"><g type="ddanda">.</g></supplied> tiruppulivalattu <hi rend="grantha">mahāde</hi>varkku malliyaṉ kārāṇai nālupaṭṭi nila morutiraḷe ya<hi rend="grantha">rcca</hi>nā<hi rend="grantha">bhoga</hi>māka vaitto mut=ta<hi rend="grantha">ra</hi>
					
					<lb n="2" break="no"/><hi rend="grantha">merucaturvvedi</hi>maṅkalatt<hi rend="grantha">u sabhai</hi>yo mitu <hi rend="grantha">pārā</hi>yaṇa<hi rend="grantha">mārgga</hi>m ve<hi rend="grantha">da</hi>mvallaṉā yu<hi rend="grantha">kta</hi>ṉākiya <hi rend="grantha">brāhma</hi>ṇaṉ <hi rend="grantha">brahmacā</hi>riyey mūṉṟupoḻutu
					
					<lb n="3"/>mā<hi rend="grantha">rādhi</hi>t tuṇpatākavu mipparicu pukkār pukkārkku mūṉṟuyāṇ ṭava<hi rend="grantha">dhi</hi>yāka vaittom va<hi rend="grantha">stu</hi> vi<hi rend="grantha">dyāma</hi>ya ṉeḻuttu <supplied reason="subaudible"><g type="ddanda">.</g></supplied>
