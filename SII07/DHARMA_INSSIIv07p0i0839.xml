					<lb n="3"/>cūṭi mīnavar nilaikeṭa villavar kulaitara Eṉai maṉṉa
					<lb n="4" break="no"/>var Iri<unclear>ta</unclear> lamarat tikkaṉaittu n=taṉ cakkara naṭātti vi<hi rend="grantha">jai</hi>A
					<lb n="5" break="no"/><hi rend="grantha">bhiṣeka</hi>m paṇṇi vi<hi rend="grantha">rasiṁhāsa</hi>nattu Avanimuḻutuṭ<hi rend="grantha">ai</hi>
					<lb n="6" break="no"/>yāḷoṭu<supplied reason="omitted">m</supplied> viṟṟirun=taruḷiya korā<hi rend="grantha">ja</hi>kecari<hi rend="grantha">panmarā</hi>na <hi rend="grantha">cakra</hi>va<unclear>r</unclear>
					<lb n="7" break="no"/>ttikaḷ <hi rend="grantha">śrī</hi>kulottuṅkacoḻa<hi rend="grantha">de</hi>varkku yāṇṭu muppatta
					<lb n="8" break="no"/>ñcāvatu <hi rend="grantha">ja</hi>yaṅkoṇṭacoḻamaṇṭalattu Oymānā<supplied reason="omitted">ṭ</supplied><unclear>ṭu</unclear>
					<lb n="9"/>ki<add place="below">ṭa</add>ṅkilāna <hi rend="grantha">rājentra</hi>coḻanallūr tiruttiṇṭī<hi rend="grantha">śvara</hi>mu
					<lb n="10" break="no"/>ṭaiya <hi rend="grantha">mahāde</hi>vaṟku <hi rend="grantha">rājarāja</hi>purattiṟku maṉṟāṭi <hi rend="grantha">candra</hi>
					<lb n="11" break="no"/>n peruṅkāṭanāṉa maturān=tatakakkoṉ vai
					<lb n="12" break="no"/>tta tiruṉun=tāviḷakku 1 Oṉṟukkum viṭṭa
					<lb n="13"/>pacu muppatu muppatuṅ kaikkoṇṭu <hi rend="grantha">cantrā</hi>tittavaṟ Erikka<supplied reason="omitted">k</supplied>
