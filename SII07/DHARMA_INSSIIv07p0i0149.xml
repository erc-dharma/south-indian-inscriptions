		<lb n="1"/><hi rend="grantha"><seg rend="check">ḷa</seg> svasti śrīḥ <supplied reason="subaudible"><g type="ddanda">.</g></supplied> tribhu</hi>vanaṉa<hi rend="grantha">ccakra</hi>va<hi rend="grantha">rtti</hi>
		<lb n="2" break="no"/>kaḷ <hi rend="grantha">śrīrājarājade</hi>vaṟku yāṇ
		<lb n="3" break="no"/>ṭu <num value="14"><g type="numeral">10</g> 4</num><g type="dash"/>Āvatu Uṭaiyār tiru
		<lb n="4" break="no"/>mutukunṟamuṭaiya nāyaṉāṟku pal
		<lb n="5" break="no"/>lavan kopperuñciṅkan Akam
		<lb n="6" break="no"/>paṭimutalikaḷil Etirikaṇāyan po
		<lb n="7" break="no"/>⌉ttappiccoḻan Inṉāyanā
		<lb n="8" break="no"/>⌉ṟku vaitta tirununtāviḷa
		<lb n="9" break="no"/>kku Onṟukku nāḷonṟuk
		<lb n="10" break="no"/>ku tevāciriyaṉāl Aḷakkum ⌈
		<lb n="11"/>ney Uḻakkukkut tiṭṭaicāt
		<lb n="12" break="no"/>tan tevāciriyakkonvacam viṭ
		<lb n="13" break="no"/>ṭa Āṭu toṇṇūṟṟāṟu <hi rend="grantha">candrā</hi>
		<lb n="14" break="no"/><hi rend="grantha">ditya</hi>va<hi rend="grantha">T</hi> cellappaṇṇuvatu
		<lb n="15"/><hi rend="grantha">panmāheśvara rakṣai</hi> <g type="gomutraFinal">.</g>
