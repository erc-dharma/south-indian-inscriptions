	<p>
		<lb n="1"/><hi rend="grantha">svasti śrī</hi> <supplied reason="subaudible"><g type="ddanda">.</g></supplied> kopperuñciṅkatevaṟkku yāṇṭu <num value="20">2 <g type="numeral">10</g></num><g type="dash"/>tāvatu <hi rend="grantha">sā</hi>n=tāni<hi rend="grantha"/>kacaivā<hi rend="grantha">cāryya</hi> kāñcikkuṟi tiruvekampamuṭaiyārāṉa 
		<lb n="2"/>Ekāmpirapaṭṭar Uṭaiyār tiruvottūruṭaiya nāyanā<supplied reason="omitted">r</supplied>kku vaitta tirununtāviḷakku Oṉṟukku pacu muppattuIraṇṭukku I
		<lb n="3" break="no"/>kkoyil tiruvu<supplied reason="omitted">ṇ</supplied>ṇāḻikai <hi rend="grantha">sabhai</hi>yom Ivarpakkal kai<surplus>y</surplus><supplied reason="omitted">k</supplied>koṇṭa poṉ Irupattunāṟkaḻañcu Ippoṉ Irupattunāṟ
		<lb n="4" break="no"/>kaḻañcum kaiyaṟa kaiy<supplied reason="omitted">k</supplied>koṇṭu Ittiruṉuntāviḷakku Oṉṟum <hi rend="grantha">candrādity</hi>aivarai celuttakkaṭavom tiruvuṇṇāḻikai
		<lb n="5"/><hi rend="grantha">sabhai</hi>yom Ivai tillaināyakapaṭṭaṉ Eḻuttu Ivai catā<hi rend="grantha">śi</hi>vapaṭṭaṉ Eḻuttu Ivai tiruvikkarapaṭṭaṉ Eḻuttu Ivai <hi rend="grantha">de</hi>va<hi rend="grantha">śikhāma</hi>ṇi<hi rend="grantha"/>
		<lb n="6" break="no"/>paṭṭaṉ Eḻuttu Itu <hi rend="grantha">śrīmāhe</hi>cu<hi rend="grantha">ra rakṣai</hi> <supplied reason="subaudible"><g type="ddanda">.</g></supplied>
