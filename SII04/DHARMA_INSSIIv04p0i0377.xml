            <div type="edition" xml:lang="tam-Latn" rendition="class:tamil maturity:vernacular">
                
                <p>
                    <lb n="1"/><hi rend="grantha">svasti śrī</hi> <supplied reason="undefined"><g type="ddanda">.</g></supplied> IraṭṭapāṭiEḻaraiyilakkamu<unclear>ṅ</unclear> koṇṭu perāṟṟaṅkaraik koppat<unclear>tā</unclear><hi rend="grantha"><unclear>ha</unclear></hi><unclear>vama</unclear>
                    <lb n="2" break="no"/>llaṉai yañcuvittavaṉāṉaiyuṅ kutiraiyuṅ kaikkoṇṭu vi<hi rend="grantha">jai</hi>ya A<hi rend="grantha">bhiṣe</hi>kañ ceytu vī<hi rend="grantha">rasi</hi><hi rend="grantha"><unclear>ṁhāsa</unclear></hi><unclear>na</unclear>
                    <lb n="3" break="no"/>ttu vīṟṟiruntaruḷiya kopparake<hi rend="grantha">śa</hi>riva<hi rend="grantha">nma</hi>rāṉa Uṭaiyār <hi rend="grantha">śrīrājentrade</hi><unclear>varkku yāṇ</unclear>
                    <lb n="4" break="no"/>ṭu Oṉpatāvatu <hi rend="grantha">ja</hi>yaṅkoṇṭacoḻamaṇṭalattu Āmūrk<unclear>koṭṭat</unclear>
                    <lb n="5" break="no"/>tu Āmūrnāṭṭu nakaramāmallapuramāṉa <hi rend="grantha">ja</hi>ṉanātapurattu nakarattomu<unclear>m</unclear> <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="6"/>maiyomum Eḻuttu Eṅkaḷūr <hi rend="grantha">śrīparameśvaramahā</hi>va<hi rend="grantha">rāha</hi>vi<hi rend="grantha">ṣṇugṛha</hi>ttāḻva <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="7"/>na Iṟaiyiliyāka muṉpuṭaiya nilamuṭpaṭak kuṭutta nilattukkuk kiḻpāṟkellai cī <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="8"/>meṟku teṉpāṟkellai Ūrakamuṭaiya <hi rend="grantha">mahā</hi>vi<hi rend="grantha">ṣṇu</hi>kkaḷ nilamāna vayaluḻāṉ <unclear>kaṇṇā</unclear> <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="9"/>melpāṟkellai kaḻikkuk kiḻakkum vaṭapāṟkellai māmallaipperumāṉkoyilukkuṅ <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="10"/><unclear>ṇ</unclear>ṇīrk kiṇaṟṟukku teṟkum Āka Iṉnāṟperellaikkum naṭuvupaṭṭa nīrnilamum <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="11"/>ṭu Uṭumpoṭi Āmai ta<unclear>va</unclear>ḻn<supplied reason="omitted">ta</supplied> te<supplied reason="omitted">v</supplied>vakaipaṭṭa nilamum Iṟaiyiḻicci Iṉnilam Itteva<unclear>ṟ</unclear>ku <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="12"/><unclear>ḷa</unclear> māmpula muḷḷi<unclear>ṭ</unclear>ṭa kaṇṇāṟu taṭi nālum <hi rend="grantha">śrī</hi>palipaṭṭiyākavum nikki niṉṟa nirnilattu <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="13"/>ṉṟukkut tiruvamutu Arici nā<unclear>nā</unclear>ḻiy<choice><sic>a</sic><corr>ā</corr></choice>ka <hi rend="grantha">sa</hi>n=ti yiraṇṭuk karici kuṟuṇikku Aiñciraṇṭāka nellu <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="14"/>murtu Iraṇṭum neyyamutu Iruceviṭaraiyum tayiramutu Uriyum Aṭaikkāyamutu <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="15"/>nellu nānāḻiyāka <hi rend="grantha">sa</hi>n=ti Iraṇṭukku nelluk kuṟuṇiyum Āka <hi rend="grantha">sa</hi>n=ti Iraṇṭukku ne<unclear>l</unclear> <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="16"/>vatākavum Innilam Uḻu<unclear>ṅ</unclear> kuṭikaḷaiyum Ittevar maṭaiviḷākat tiruppāraiyum Ep<seg type="aksara"><seg type="component" subtype="body"><gap reason="lost" quantity="1" unit="component"/></seg>e</seg> <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="17"/>vum Ipparicu Icain=tu tevatāna Iṟaiyiliyākak kuṭuttom māma<supplied reason="omitted">l</supplied>lapuramāṉa <hi rend="grantha">ja</hi>nanātapura <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="18"/>Iva<supplied reason="omitted">r</supplied>kaḷ colla Eḻutineṉ Ivvūrk karaṇattāṉ tiruveḻicciluṭaiyāṉ toṇṭa <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="19"/>vai Oḷinākaṉ mātaiyāṉa Aḻakiyacoḻa Āmurnāṭṭu muventaveḷāṉ Eḻut <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="20"/>ttu Ivai Oḷinākaṉ nārāyaṇaṉ Eḻuttu Ivai kaḷattuḻāṉ cūṟṟi Araici<unclear>n</unclear>eḻuttu I <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="21"/>vai Iṇṭipuṟavaṉ ca<unclear>ṅ</unclear>kanākaṉ Eḻuttu Ivai māppūti nārāyaṇaṉ mātakkali Eḻuttu <unclear>I</unclear>vai <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="22"/>Uttaraṅkiḻavaṉ mukalinākaṉ Eḻuttu Ivai vaṇṭāḻañceriyuṭaiyāṉ Araiyaṉ picca<surplus>ṉ</surplus> <unclear>ne</unclear>ḻut<unclear>tu</unclear> <gap reason="lost" quantity="5" unit="character"/> 
                    <lb n="23"/>k kumariyiṭaic ceytār ceyta pāvaṅ koḷvāṉ <supplied reason="undefined"><g type="ddanda">.</g></supplied>
                </p>
                
