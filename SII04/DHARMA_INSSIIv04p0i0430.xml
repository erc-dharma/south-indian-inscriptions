            <div type="edition" xml:lang="tel-Latn" rendition="class:grantha maturity:vernacular">
                
                <p>
                    <lb n="1"/><gap reason="lost" quantity="10" unit="character"/> <hi rend="grantha">śvara kākatī</hi>ya <hi rend="grantha">pratāparudrade</hi>va<hi rend="grantha">mahārāja</hi><seg rendition="class:tamil maturity:vernacular">lu</seg>
                    <lb n="2"/><gap reason="lost" quantity="10" unit="character"/> <hi rend="grantha">mugānu pṛthi</hi>vī<hi rend="grantha">rājyaM seyucuṇḍagānu Ārāju</hi>
                    <lb n="3"/><gap reason="lost" quantity="10" unit="character"/> <hi rend="grantha">śrīmatkākatarā</hi>ya <hi rend="grantha">sthāpanācāryasvāmi<seg type="aksara"><seg type="component" subtype="body"><gap reason="lost" quantity="1" unit="component"/></seg>e</seg></hi>
                    <lb n="4" break="no"/><gap reason="lost" quantity="6" unit="character"/> <hi rend="grantha">gāri</hi> <seg rendition="class:tamil maturity:vernacular">koṭu<unclear>k</unclear>ku</seg> <hi rend="grantha">de</hi>va<hi rend="grantha">ri</hi>nāyani<hi rend="grantha">gāru dakṣi</hi>
                    <lb n="5" break="no"/><gap reason="lost" quantity="10" unit="character"/> <hi rend="grantha">pacci Enugulu mohariñci</hi>na <hi rend="grantha">poṭṭilāṭṭa</hi>
                    <lb n="6" break="no"/><gap reason="lost" quantity="7" unit="character"/> <hi rend="grantha">ti</hi><seg rendition="class:tamil maturity:vernacular">ruva</seg><hi rend="grantha"><unclear>di</unclear>kuṇḍrāmu</hi><seg rendition="class:tamil maturity:vernacular">vīra</seg><hi rend="grantha">kṣetrānanu</hi> <seg rendition="class:tamil maturity:vernacular">vīra</seg><hi rend="grantha">pāṇḍyani</hi>n=ni <seg rendition="class:tamil maturity:vernacular">malaiyāḷa tiruvaṭi</seg>
                    <lb n="7" break="no"/><gap reason="lost" quantity="10" unit="character"/> <hi rend="grantha">nukulānu sarvaśrījiniśva</hi><seg rendition="class:tamil maturity:vernacular">ni cuntarapā</seg>
                </p>
                
