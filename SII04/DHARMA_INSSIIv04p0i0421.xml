            <div type="edition" xml:lang="tam-Latn" rendition="class:tamil maturity:vernacular">
                
                <p>
                    <lb n="1"/><hi rend="grantha">svastiśrī</hi> <g type="circleDouble">.</g> <supplied reason="undefined"><g type="ddanda">.</g></supplied> <hi rend="grantha">svasti samascabhu</hi>vanā<hi rend="grantha">pra</hi>ya <hi rend="grantha">śrīpṛthvi</hi>va<hi rend="grantha">llabha mahārājādhirāja parameśvara</hi>
                    <lb n="2" break="no"/><hi rend="grantha">dvāra</hi>vati<hi rend="grantha">pura</hi>va<hi rend="grantha">rādhiśvara yāda</hi>va<hi rend="grantha">kulāṃbara<choice><sic>dvi</sic><corr>dyu</corr></choice>ma</hi>ṇi <hi rend="grantha">sarvajñacūḍāma</hi>ṇi <hi rend="grantha">malarājarāja ma</hi>
                    <lb n="3" break="no"/><hi rend="grantha">laparuḷugaṇḍa kada</hi>na<hi rend="grantha">pracaṇḍa gaṇḍabheruṇḍa</hi> ne<hi rend="grantha">kāṃga</hi>vi<hi rend="grantha">ra</hi> na<hi rend="grantha">sahā</hi>ya<hi rend="grantha">śūra śa</hi>nivā<hi rend="grantha">ra siddhi giridurggama</hi>
                    <lb n="4" break="no"/><hi rend="grantha">llacaladaṃkarāma vairibhakaṇṭhira</hi>va <hi rend="grantha">magadharājya</hi>ni<hi rend="grantha">rmmūla</hi>na <hi rend="grantha">pāṇḍyakulasa<seg rend="check"><supplied reason="omitted">mu</supplied></seg>ḍḍhara</hi>ṇa <hi rend="grantha">coḷarājyapra</hi>ti
                    <lb n="5" break="no"/><hi rend="grantha">ṣṭhācāryya</hi> ni<hi rend="grantha">ssaṃgapra</hi>tā<hi rend="grantha">pacakra</hi>va<hi rend="grantha"><supplied reason="omitted">r</supplied>ttipośaḷa śrī</hi>vī<hi rend="grantha">rasomiśvara de</hi>va<hi rend="grantha">rasar·</hi> Uṭaiyār tiruvāṉai
                    <lb n="6" break="no"/>kkāvuṭaiyār koyiṟtāṉattāṟkku nam Ammai kā<unclear>ḷa</unclear>lateviyāṟkku naṉṟāka nām Eḻun<surplus>ta</surplus>
                    <lb n="7" break="no"/>taruḷuvitta Uṭaiyār pocaḷī<hi rend="grantha">śvara</hi>muṭaiyārkku Irupatteḻāvatu kārmutal nām Iṭṭa
                    <lb n="8" break="no"/>tavatāṉaṅkaḷil Uṭal koṇṭu Iṉṉā<unclear>ya</unclear>ṉāṟkkup pūcai tāṅkaḷ celuttuvārkaḷākac
                    <lb n="9" break="no"/>coṉṉom Ippaṭiceyka <seg rend="check"><tlka>Te-Ka phrase</tlka></seg> I<unclear>tu</unclear> tevartiru Eḻuttu <supplied reason="undefined"><g type="ddanda">.</g></supplied>
                </p>
                
