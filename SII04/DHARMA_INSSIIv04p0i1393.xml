            <div type="edition" xml:lang="tam-Latn" rendition="class:tamil maturity:vernacular">
                
                <div type="textpart" n="A"><head>A.</head>
                
                <p>
                    <lb n="1"/><hi rend="grantha">śrī</hi>moka
                    <lb n="2" break="no"/>nūruṭai
                    <lb n="3" break="no"/>yāṉ tiru
                    <lb n="4" break="no"/>ppūvaṇa
                    <lb n="5" break="no"/>tevaṉ <supplied reason="undefined"><g type="ddanda">.</g></supplied>
                </p>
                
                </div>
                
                <div type="textpart" n="A"><head>B.</head>
                
                <p>
                    <lb n="1"/><hi rend="grantha">śrī</hi> tillai
                    <lb n="2" break="no"/>kkaracu
                    <lb n="3"/>tiyākacin
                    <lb n="4" break="no"/>tāmaṇimu
                    <lb n="5" break="no"/>ventave
                    <lb n="6"/>ḷān <supplied reason="undefined"><g type="ddanda">.</g></supplied>
                </p>
                
                </div>
                
                <div type="textpart" n="A"><head>C.</head>
                
                <p>
                    <lb n="1"/><hi rend="grantha">śrī</hi>mukarināṭā
                    <lb n="2" break="no"/>ḻvān maka
                    <lb n="3" break="no"/>ḷ kaṟpakam <supplied reason="undefined"><g type="ddanda">.</g></supplied>
                </p>
                
                </div>
                
                <div type="textpart" n="A"><head>D.</head>
                
                <p>
                    <lb n="1"/><hi rend="grantha">śrī</hi>nallūru
                    <lb n="2" break="no"/>ṭaiyān pa
                    <lb n="3" break="no"/>ñcanetivā
                    <lb n="4" break="no"/>ṇan <supplied reason="undefined"><g type="ddanda">.</g></supplied>
                </p>
                </div>
                
