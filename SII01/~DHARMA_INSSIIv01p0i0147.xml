                <div n="A" type="textpart"><head>First inscription.</head>
                    
                    <p>
                        <lb n="A1"/>kovirā<hi rend="grantha">ja</hi>kecaripa<hi rend="grantha"><unclear>nma</unclear></hi><unclear>r</unclear>kku yā<unclear>ṇ</unclear>ṭu <gap reason="lost" quantity="10" unit="character"/> 
                        <lb n="A2"/>tūro<unclear>m</unclear> kai Eḻuttu <gap reason="lost" quantity="10" unit="character"/> 
                        <lb n="A3"/>ṭu kaṭava poṉ tu<unclear>ḷai</unclear> niṟai<unclear>yiṉ</unclear> pati<unclear>ṉai</unclear>ṅka<unclear>ḻa</unclear>ñcukkum k<unclear>ā</unclear> <gap reason="lost" quantity="10" unit="character"/> 
                        <lb n="A4"/><hi rend="grantha">hāde</hi>vaṟkku can<hi rend="grantha">tra</hi>rātitta<unclear>varai</unclear> nicatam Uḻakkeṇ<unclear>ṇai</unclear> <gap reason="lost" quantity="10" unit="character"/> 
                        <lb n="A5"/>yo<unclear>ru</unclear> n<unclear>o</unclear>ntāviḷakki<unclear>nu</unclear>kku Eṇṇai Aṭṭuvo<unclear>m</unclear> <gap reason="lost" quantity="10" unit="character"/> 
                        <lb n="A6"/><unclear>Iv</unclear>vūr kūṭṭat<unclear>tāṉ</unclear> māṉatoṅkaṉmalleru<supplied reason="undefined">māṉeṉ</supplied> <supplied reason="subaudible"><g type="ddanda">.</g></supplied>
                    </p>
                    
