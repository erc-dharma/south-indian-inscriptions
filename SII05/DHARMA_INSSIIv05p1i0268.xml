<div type="edition" xml:lang="tam-Latn" rendition="class:tamil maturity:83215">

<p>

	<lb n="1"/><hi rend="grantha">svastiśrī <supplied reason="undefined"><g type="ddanda">.</g></supplied> tribhu</hi>vanaccakkaravattikaḷ

	<lb n="2"/><hi rend="grantha">śrī</hi>virarāce<hi rend="grantha">ntra</hi> tevaṟku yāṇṭu pa

	<lb n="3" break="no"/>tinmunṟāvatu poṅkalūkkānāṭṭu

	<lb n="4"/>kiranūr Āḻuṭaiyār tiruvā<unclear>ki</unclear><hi rend="grantha"><unclear>śva</unclear></hi><unclear>ramu</unclear>⌈

	<lb n="5" break="no"/>ṭaiyāṟku <hi rend="grantha">sa</hi>n=tiyātipam munṟukku <unclear>tu</unclear>ḷai

	<lb n="6" break="no"/>mu<surplus>ka</surplus>kkaḻañcu pon <hi rend="grantha">śrī</hi>paṇṭāra<unclear>ttile</unclear> Oṭuk

	<lb n="7" break="no"/>kinen tillai<unclear>ppirānpa</unclear>ṭṭar piḷḷaika<unclear>ḷil</unclear> piccan

	<lb n="8"/>Akoraci<unclear>vaneṉ</unclear> <gap reason="lost" quantity="3" unit="character"/> tuḷai mukkaḻañcu p<unclear>o</unclear>

	<lb n="9" break="no"/><unclear>n</unclear> kaikko<unclear>ṇṭom Ikkoyilil civa</unclear><hi rend="grantha">brāhma</hi>

	<lb n="10" break="no"/>ṇar kāci<unclear>paṉ pāṇṭiyataṉañciyacai</unclear>vecuramu

	<lb n="11" break="no"/>ṭaiyānāṉa <hi rend="grantha">ś</hi> Ai<hi rend="grantha">vva</hi> ca<hi rend="grantha">kra</hi> vatti Uḷḷiṭṭomum Āṇṭa

	<lb n="12" break="no"/>piḷḷai Ariyavanāna <hi rend="grantha">ś</hi> Ai<hi rend="grantha">vva</hi> kalālaiyaca<hi rend="grantha">kra</hi> vatti <unclear>U</unclear>ḷ

	<lb n="13" break="no"/>ḷiṭṭomum kuṭaṅkoṭu koyilpukuvār Ivviḷakku

	<lb n="14"/>Erippārāka pa<hi rend="grantha">nmāheśvararakṣai <supplied reason="undefined"><g type="ddanda">.</g></supplied></hi> 
